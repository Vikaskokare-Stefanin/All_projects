{
  "name": "FullStack",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.15.7",
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "cds-plugin-ui5": "^0.13.0",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-fullui": "cds watch --open fullui.com.fullui/index.html?sap-ui-xx-viewCache=false --livereload false",
    "undeploy": "cf undeploy FullStack --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "private": true,
  "cds": {
    "requires": {
      "db": {
        "kind": "hana"
      },
      "auth": {
     
          "kind": "mocked",
          "users": {
            "vikas": {
              "password": "vikas",
              "roles": [
                "Admin"
              ]
            },
             "raj": {
              "password": "raj",
              "roles": [
                "Student"
              ]
            },
            "pavan": {
               "password": "pavan",
              "roles": [
                "Teacher"
              ]
            }
          }
        
      },
      "connectivity": true,
      "destinations": true
    }
  },
  "workspaces": [
    "app/*"
  ]
}
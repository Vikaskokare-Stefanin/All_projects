  {
    "name": "Security",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "dependencies": {
      "@sap/cds": "^9",
      "express": "^4",
      "@sap/xssec": "^4",
      "@cap-js/postgres": "^2"
    },
    "devDependencies": {
      "@cap-js/sqlite": "^2",
      "@sap/cds-dk": "^9",
      "cds-plugin-ui5": "^0.13.0",
      "rimraf": "^5.0.5",
      "mbt": "^1.2.29"
    },
    "scripts": {
      "start": "cds-serve",
      "watch-uiapp": "cds watch --open uiapp.com.uiapp/index.html?sap-ui-xx-viewCache=false --livereload false",
      "undeploy": "cf undeploy Security --delete-services --delete-service-keys --delete-service-brokers",
      "build": "rimraf resources mta_archives && mbt build --mtar archive",
      "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
    },
    "private": true,
    "cds": {
      "requires": {
        "[production]": {
          "auth": "mock"
        },
        "[development]": {
          "strategy": "mock",
          "users": {
            "vikas@gmail.com": {
              "password": "vikas",
              "roles": [
                "Manager_Role"
              ]
            },
            "pavan@gmail.com": {
              "password": "pavan",
              "roles": [
                "Employee_Role"
              ]
            }
          }
        },
        "connectivity": true,
        "destinations": true,
        "html5-repo": true
      }
    },
    "workspaces": [
      "app/*"
    ],
    "sapux": [
      "app/uiapp"
    ]
  }
